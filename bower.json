{
    "name": "angular-sandbox",
    "version": "1.0.0",
    "license": "MIT",
    "private": true,
    "ignore": [
        "**/.*",
        "node_modules",
        "bower_components"
    ],
    "dependencies": {
        "angular": "~1.3.15",
        "angular-ui-router": "~0.2.13",
        "angular-bootstrap": "~0.12.1",
        "bootstrap": "~3.3.4"
    },
    "overrides": {
        "angular": {
            "main": {
                 "development": "angular.js",
                "production": "angular.min.js"
            },
            "dependencies": {
                "jquery": "~2.1.3"
            }
        },
        "angular-ui-router": {
            "main":{
                "development": "release/angular-ui-router.js",
                "production": "release/angular-ui-router.min.js"
            }
        },
        "angular-bootstrap" : {
            "main":{
                "development": "ui-bootstrap-tpls.js",
                "production": "ui-bootstrap-tpls.min.js"
            }
        },
        "jquery": {
            "main":{
                "development": "dist/jquery.js",
                "production": "dist/jquery.min.js"
            }
        }
    },
    "devDependencies": {
        "angular-mocks": "~1.3.15"
    }
}
